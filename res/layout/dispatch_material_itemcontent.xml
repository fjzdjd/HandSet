<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_bg"
    android:orientation="vertical" >

    <include layout="@layout/head_second_level_layout" />

    <TextView
        android:id="@+id/item_content_txt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="入库任务单内容列表"
        android:textColor="@color/radio_color_pressed"
        android:textSize="18sp" />

    <include layout="@layout/bottom_button_layout" />

    <include layout="@layout/item_layout" />

    <ListView
        android:id="@+id/dispatch_airMaterialEnterItem_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="2dip" >
    </ListView>

</LinearLayout>